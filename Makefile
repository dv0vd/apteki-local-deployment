build:
	@echo '\n============================================================'
	@echo '---------------- Building containers------------------------'
	@echo '============================================================\n'

	docker load < ./_/docker_images/alpine-3.16.0.tar
	docker load < ./_/docker_images/bitnami-clickhouse-24.4.1.tar
	docker load < ./_/docker_images/elasticsearch-7.17.1.tar
	docker load < ./_/docker_images/golang-1.18-alpine.tar
	docker load < ./_/docker_images/minio-RELEASE.2024-05-07T06-41-25Z.fips.tar
	docker load < ./_/docker_images/mongo-7.0.9.tar
	docker load < ./_/docker_images/postgres-16.2.tar
	docker load < ./_/docker_images/rabbitmq:3.11.2-management.tar
	docker load < ./_/docker_images/redis:7.2.4.tar
	docker load < ./_/docker_images/ubuntu-bionic.tar
	docker load < ./_/docker_images/ubuntu-jammy.tar
	docker load < ./_/docker_images/unit-php.tar
	docker load < ./_/docker_images/bitnami-redis-cluster-7.2.4.tar
	docker load < ./_/docker_images/willfarrell-autoheal-1.2.0.tar
	docker load < ./_/docker_images/node-16.15.1.tar
	docker load < ./_/docker_images/node-20-slim.tar
	docker load < ./_/docker_images/node-18-slim.tar
	docker load < ./_/docker_images/kibana-7.17.1.tar


ifeq ($(c),null)
	docker compose build
endif
ifneq ($(c),null)
	docker compose build $(c)
endif


rebuild:
	@$(MAKE) stop
	@$(MAKE) build
	@$(MAKE) start


start:
ifeq ($(c),import-prepare)
	@echo '\n============================================================'
	@echo '----- Error! Use make import-prepare command instead! ------'
	@echo '============================================================\n'
else ifeq ($(c),import-deploy)
	@echo '\n============================================================'
	@echo '----- Error! Use make import-deploy command instead! ------'
	@echo '============================================================\n'
else
	@echo '\n============================================================'
	@echo '---------------- Starting containers------------------------'
	@echo '============================================================\n'

	mkdir -p ./_/data/elasticsearch
	mkdir -p ./_/data/clickhouse
	$(MAKE) clear-logs
ifeq ($(c),null)
	docker compose up -d --remove-orphans --wait
endif
ifneq ($(c),null)
	docker compose up -d $(c) --remove-orphans --wait
endif
endif
# docker compose up -d $(c) --remove-orphans --force-recreate --always-recreate-deps --wait


stop:
	@echo '\n============================================================'
	@echo '---------------- Stopping containers------------------------'
	@echo '============================================================\n'
ifeq ($(c),null)
	docker compose down
endif
ifneq ($(c),null)
	docker compose down $(c)
endif


restart: stop start


start-laravel-services:
	$(MAKE) start c='$(LARAVEL_SERVICES)'



stop-laravel-services:
	$(MAKE) stop c='$(LARAVEL_SERVICES)'



restart-laravel-services: stop-laravel-services start-laravel-services



start-go-services:
	@for s in $(GO_SERVICES); do \
		$(MAKE) start c=$$s
	done


stop-go-services:
	@for s in $(GO_SERVICES); do \
		$(MAKE) stop c=$$s
	done


restart-go-services: stop-go-services start-go-services


restart-front-services: stop-front-services start-front-services


start-backend:
	$(MAKE) start-laravel-services
	$(MAKE) start-go-services
	$(MAKE) start c=redirect-service

stop-backend:
	$(MAKE) stop-laravel-services
	$(MAKE) stop-go-services
	$(MAKE) stop c=redirect-service


restart-backend: stop-backend start-backend


start-frontend:
	$(MAKE) start c=front-service


stop-frontend:
	$(MAKE) stop c=front-service


restart-frontend: stop-frontend start-frontend


refresh:
	@$(MAKE) stop
	@$(MAKE) build
	@$(MAKE) master
	@$(MAKE) composer
# @$(MAKE) composer-no-deps
	@$(MAKE) clear-go-cache
	@$(MAKE) clear-front-cache
	@$(MAKE) start
	@$(MAKE) queues
	@$(MAKE) migrate
	@$(MAKE) db-restore
	@$(MAKE) db-vacuum



init:
	@$(MAKE) stop
	@$(MAKE) build
	@$(MAKE) master
	@$(MAKE) dep-restore
	@$(MAKE) clear-go-cache
	@$(MAKE) clear-front-cache
	@$(MAKE) start
	@$(MAKE) queues
	@$(MAKE) db-restore
	@$(MAKE) db-vacuum



composer:
	@echo '\n============================================================';
	@echo '--------- Installing composer dependencies -----------------';
	@echo '============================================================\n';

ifeq ($(c),null)
	@$(MAKE) start-laravel-services

	@for s in $(LARAVEL_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		docker exec -it $$s php /tmp/composer.phar i; \
	done

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';
endif
ifneq ($(c),null)
	$(MAKE) start

	@for s in $(c); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		docker exec -it $$s php /tmp/composer.phar i; \
	done

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';
endif


