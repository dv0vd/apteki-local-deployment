YELLOW='\033[1;33m'
PURPLE='\033[1;35m'
q=''
a=''

motivation:
	@rand=$(shell awk 'BEGIN{srand();printf("%d", 431*rand())}')
	@rand=$$(( $$rand + 1))
	@for quote in $(QUOTES); do \
		i=$$((i+1)); \
		if [ $$rand -eq $$i ]; then \
			q=$$quote; break; \
		fi
	done
	i=0
	@for author in $(AUTHORS); do \
		i=$$((i+1)); \
		if [ $$rand -eq $$i ]; then \
			a=$$author; break; \
		fi
	done
	echo ${YELLOW}$$q; \
	echo ${PURPLE}'	'$$a; \


