queues:
	@echo '\n============================================================';
	@echo '------------------ Binding queues --------------------------';
	@echo '============================================================\n';
ifeq ($(c),null)
	@$(MAKE) start-laravel-services

	@for s in $(LARAVEL_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		docker exec -it $$s php artisan event-dispatcher:queue-binding --quorum=1; \
	done

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';
endif
ifneq ($(c),null)
	$(MAKE) start

	@for s in $(c); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		docker exec -it $$s php artisan event-dispatcher:queue-binding --quorum=1; \
	done

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';
endif


master:
	@echo '\n============================================================';
	@echo '---------------- Syncing with gitlab------------------------';
	@echo '============================================================\n';

ifeq ($(c),null)
	@cd ./services;

	@for s in $(LARAVEL_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		cd $$s ; \
		git checkout master && git pull origin master ; \
		cd .. ; \
	done
	@for s in $(GO_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		cd $$s ; \
		git checkout master && git pull origin master ; \
		cd .. ; \
	done

	@for s in $(NODE_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		cd $$s ; \
		git checkout master && git pull origin master ; \
		cd .. ; \
	done

	@cd ../packages;
	@for s in $(PACKAGES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		cd $$s ; \
		git checkout master && git pull origin master ; \
		cd .. ; \
	done

	@echo '\n============================================================';
	@echo '----------- 'docs' -----------';
	@echo '============================================================\n';
	@cd ../docs;
	@git checkout master && git pull origin master ;
	@cd .. ;
endif
ifneq ($(c),null)
	@for s in $(c); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		cd $$s ; \
		git checkout master && git pull origin master ; \
		cd - ; \
	done
endif


clear-logs:
	@echo '\n============================================================';
	@echo '-------------------- Clearing logs -------------------------';
	@echo '============================================================\n';

	@for s in $(LARAVEL_SERVICES); do \
		rm -f ./services/$$s/storage/logs/laravel.log; \
		rm -f ./logs/$$s.log; \
		ln -s ../services/$$s/storage/logs/laravel.log ./logs/$$s.log; \
		echo $$s: logs have been successfully cleared
	done


clear-go-cache:
	@echo '\n============================================================';
	@echo '-------------------- Clearing go cache ---------------------';
	@echo '============================================================\n';

	@sudo rm ./_/data/go/import-prepare-cache -rf
	@sudo rm ./_/data/go/import-deploy-cache -rf
	@sudo rm ./_/data/go/stock-service-cache -rf
	@sudo rm ./_/data/go/clickhouse-analytics-service -rf

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';


clear-front-cache:
	@echo '\n============================================================';
	@echo '-------------------- Clearing Nuxt cache ---------------------';
	@echo '============================================================\n';

	$(MAKE) start c=front-service
	docker exec -it front-service npx nuxi cleanup

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';


clear-laravel-cache:
	@echo '\n============================================================';
	@echo '-------------------- Clearing Laravel cache ----------------';
	@echo '============================================================\n';

	@$(MAKE) start c=idm-service
	@docker exec -it idm-service php artisan cache:clear

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';


import-prepare:
	@docker compose up import-prepare --remove-orphans --wait --scale import-prepare=1


import-deploy:
	@docker compose up import-deploy --remove-orphans --wait --scale import-deploy=1


clear-broken-indexes:
	@docker compose up search-service-broken-indexes-cleaner --remove-orphans --wait --scale search-service-broken-indexes-cleaner=1

kibana:
	@docker compose up kibana --remove-orphans --wait --scale kibana=1

d = null
db-dump:
	@$(MAKE) start c=postgres

ifeq ($(d),null)
	@for db in $(DATABASES); do \
		echo '\n============================================================';
		echo '--------- Backing up '$$db' --------------';
		echo '============================================================\n';

		docker exec -it postgres pg_dump -v -U postgres -d $$db -F tar -f /backups/$$db.tar
	done
endif
ifneq ($(d),null)
	@for db in $(d); do \
		echo '\n============================================================';
		echo '--------- Backing up '$$db' --------------';
		echo '============================================================\n';

		docker exec -it postgres pg_dump -v -U postgres -d $$db -F tar -f /backups/$$db.tar
	done
endif
	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';


db-restore:
	@$(MAKE) start c=postgres

ifeq ($(d),null)
	@for db in $(DATABASES); do \
		echo '\n============================================================';
		echo '--------- Restoring '$$db' --------------';
		echo '============================================================\n';

		docker exec -it postgres pg_restore -v -U postgres -d $$db -F tar /backups/$$db.tar
	done
endif
ifneq ($(d),null)
	@for db in $(d); do \
		echo '\n============================================================';
		echo '--------- Restoring '$$db' --------------';
		echo '============================================================\n';

		docker exec -it postgres pg_restore -v -U postgres -d $$db -F tar /backups/$$db.tar
	done
endif
	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';


db-vacuum:
	@echo '\n============================================================';
	@echo '-------------------- Vacuuming database --------------------';
	@echo '============================================================\n';

	@$(MAKE) start c=postgres

	@for db in $(DATABASES); do \
		docker exec -it postgres psql -U postgres -d $$db -c 'vacuum full verbose'
	done


dep-dump:
	@echo '\n============================================================';
	@echo '--- --------------- Dumping dependencies -------------------';
	@echo '============================================================\n';

	@for s in $(LARAVEL_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		sudo rm ./_/dependencies/$$s -rf; \
		mkdir ./_/dependencies/$$s -p; \
		sudo cp ./services/$$s/vendor/ ./_/dependencies/$$s -r;
	done

	@for s in $(NODE_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		sudo rm ./_/dependencies/$$s -rf; \
		mkdir ./_/dependencies/$$s -p; \
		sudo cp ./services/$$s/node_modules/ ./_/dependencies/$$s -r;
	done

	@for s in $(GO_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		sudo rm ./_/dependencies/$$s -rf; \
		mkdir ./_/dependencies/$$s -p; \
		sudo cp ./_/data/go/$$s-pkg/ ./_/dependencies/$$s -r;
	done


dep-restore:
	@echo '\n============================================================';
	@echo '------------------- Restoring dependencies -----------------';
	@echo '============================================================\n';

	@for s in $(LARAVEL_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		sudo rm ./services/$$s/vendor -r; \
		sudo cp ./_/dependencies/$$s/vendor ./services/$$s -r;
	done

	@for s in $(NODE_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		sudo rm ./services/$$s/node_modules -r; \
		sudo cp ./_/dependencies/$$s/node_modules ./services/$$s -r;
	done

	@for s in $(GO_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		sudo rm ./_/data/go/$$s-pkg/ -r; \
		sudo cp ./_/dependencies/$$s/$$s-pkg ./_/data/go/$$s-pkg -r;
	done



