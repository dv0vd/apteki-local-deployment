migrate:
	@echo '\n============================================================';
	@echo '---------------- Running migrations ------------------------';
	@echo '============================================================\n';

ifeq ($(c),null)
	@$(MAKE) start-laravel-services;

	@for s in $(LARAVEL_SERVICES); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		docker exec -it $$s php artisan migrate --force; \
	done

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';
endif
ifneq ($(c),null)
	$(MAKE) start

	@for s in $(c); do \
		echo '\n============================================================'; \
		echo '----------- '$$s' -----------';\
		echo '============================================================\n'; \

		docker exec -it $$s php artisan migrate --force; \
	done

	@echo '\n============================================================';
	@echo '------------------------ Success! --------------------------';
	@echo '============================================================\n';
endif


